{
  "$schema": "https://schema.tauri.app/config/2",
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "createUpdaterArtifacts": false,
    "windows": {
      "signCommand": {
        "cmd": "cmd",
        "args": ["/C", "C:\\windowsdisk\\finceptTerminal\\fincept-terminal-desktop\\src-tauri\\sign.bat", "%1"]
